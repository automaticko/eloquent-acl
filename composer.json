{
    "name": "automaticko/eloquent-acl",
    "description": "Laravel acl through users, roles and permissions",
    "keywords": [
        "laravel",
        "eloquent",
        "acl",
        "roles",
        "permissions"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "automaticko",
            "email": "automaticko@gmail.com"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "autoload": {
        "psr-4": {
            "Automaticko\\ACL\\": "src"
        }
    },
    "require": {
        "ultraware/roles": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "orchestra/testbench": "~3.4.5",
        "orchestra/database": "~3.4.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
